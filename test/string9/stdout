removePrefix('abc.defghi', 'abc.') -> 'defghi'
removePrefix('abc.defghi', 'a') -> 'bc.defghi'
removePrefix('abc.defghi', 'abd.') -> 'abc.defghi'
removePrefix('abc.defghi', 'ghi') -> 'abc.defghi'
removePrefix('abc.defghi', 'abc.defghi') -> ''
removePrefix('abc.defghi', '') -> 'abc.defghi'
removeSuffix('abcdef.ghi', '.ghi') -> 'abcdef'
removeSuffix('abcdef.ghi', 'i') -> 'abcdef.gh'
removeSuffix('abcdef.ghi', '.ghj') -> 'abcdef.ghi'
removeSuffix('abcdef.ghi', 'abc') -> 'abcdef.ghi'
removeSuffix('abcdef.ghi', 'abcdef.ghi') -> ''
removeSuffix('abcdef.ghi', '') -> 'abcdef.ghi'
