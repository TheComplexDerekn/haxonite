// Test the length(String) and ulength(String) functions.

module string9 is

  func run(s: String) is
    var n1 = #s;
    var n2 = length(s);
    var n3 = ulength(s);
    write($"{n1:2} {n2:2} {n3:2} '{s}'\n");
  end

  func main() is
    run("hello world");
    run("");
    run("aaâézz");
  end

end
