// Test character literals.

module char1 is

  func main() is
    var s = "The quick brown fox_";
    var i = 0;
    while i < byteLength(s) do
      var c = codepoint(s, i);
      var cc = c;
      if c == ' ' then
        cc = '*';
      elseif c == '_' then
        cc = '\n';
      end
      write($"{cc:c}");
      i = nextCodepoint(s, i);
    end
  end

end
