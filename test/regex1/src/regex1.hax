// Test the regular expression reTest() function.

module regex1 is

  func main() is
    doTest("a+b+", "xxaabyy");
    doTest("a+b+", "xxxaaybbz");
    doTest("(a+b+", "xxxaaybbz");
    // invalid regex
    doTest("*", "abc*def");
  end

  func doTest(re: String, s: String) is
    var tRes = reTest(re, s);
    if (ok(tRes)) then
      var t = tRes!;
      write($"reTest('{re}', '{s}') -> {t}\n");
    else
      write($"reTest('{re}', '{s}') -> invalid regex\n");
    end
  end

end
