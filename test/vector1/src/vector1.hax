module vector1 is

  public func main() is
    var v = new Vector[Int];
    var i = 1;
    while i <= 20 do
      append(v, i*10);
      i = i + 1;
    end

    write("A:");
    i = 0;
    while i < length(v) do
      var x = get(v, i);
      write($" {i}:{x}");
      i = i + 1;
    end
    write("\n");

    delete(v, 10);

    write("B:");
    i = 0;
    while i < length(v) do
      var x = get(v, i);
      write($" {i}:{x}");
      i = i + 1;
    end
    write("\n");

    while length(v) > 0 do
      delete(v, length(v) - 1);
    end

    write("C: ");
    i = 0;
    while i < length(v) do
      var x = get(v, i);
      write($" {i}:{x}");
      i = i + 1;
    end
    write("\n");
  end

end
